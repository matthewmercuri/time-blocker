{
  "extends": [
    "next/core-web-vitals",
    "plugin:react/recommended",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "prettier/prettier": "warn",
    "react/react-in-jsx-scope": "off",
    "react/self-closing-comp": "error"
  }
}